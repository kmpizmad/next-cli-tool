    "type": "git"
  },
  "contributors": [
    "kmpizmad"
  ],
  "author": {
    "email": "nagy.viktor392@gmail.com",
    "name": "Nagy Viktor"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": "bin/index.js",
  "engines": {
    "npm": "use Yarn!",
